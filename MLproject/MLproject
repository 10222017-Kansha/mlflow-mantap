name: tutorial

conda_env: conda.yaml

entry_points:
  main:
    parameters:
      alpha: {type: float, default: 0.5}
      l1_ratio: {type: float, default: 0.1}
      dataset : {type: string, default: "wine_quality_preprocessing.csv"}
    command: "python modelling.py {alpha} {l1_ratio} {dataset}"
